<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN"      
 "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.wp.netctoss.dao.AdminDAO">
	<!-- EmpDAO中有方法save()与id="save"对应 -->
	<select id="findByAdminCode" parameterType="com.wp.netctoss.entity.Admin" resultType="com.wp.netctoss.entity.Admin">
	select * from Admin_info_wp where admin_code=id=#{adminCode}
	</select>
	

	
</mapper>







